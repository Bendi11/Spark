
type test<T> = {
    T first,
    i32 second
}

type none = ()
type opt<T> = T | none
type identity<T> = fun(T) -> T

fun ext puts(*u8)

type test_with_i32<T> = test:<T> | opt:<T>

fun testing<T>() -> *T {
    puts("Hello from generic function")
    return $*T 0u64
}

fun testing<T>:<i32>() -> *i32 {
    puts("Hello from i32 function")
    return $*i32 0u64
}

fun ext main(i32 argc, **u8 argv) -> i32 {
    let d = $identity:<i32> testing:<i32>
    let c = d(100)
    let a = #test:<i32> {
        first = 31,
        second = 1
    }

    testing:<i32>()
    testing:<u64>()
    testing:<i32>()
    return 0
}
