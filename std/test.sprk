use std::str
use std::io
use std::io::fs

fun main() i32 {
    let str::Str str = str::str_new();

    str::write_to_stdout(&str);
    io::puts("Hello World!!!!!\n");

    let u8 ptr a = {u8 ptr}0;
    let u8 ptr b = a + 100u64;

    let str::Str file_name = str::str_from("test.txt");
