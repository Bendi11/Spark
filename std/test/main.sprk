use std::io::fs
use std::string::null
use std::io::term

fun main() i32 {
    term::puts("Hello World!!!");
    let file = fs::open("./test.txt", fs::opts_default());
    if file == {fs::File ptr}0 {
        ret -1;
    };

    let written = {i32}fs::write(file, "Testing\n", null::strlen("Testing\n"));
    fs::close(file);
    ret written;
}